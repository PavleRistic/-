KK=1;EE=0;ae=0;ea=1
def Gl(r,d,o,l):
    global KK
    if M[r]==1 or M[d]==1 or M[o]==1 or M[l]==1:KK=0;
    M[r]=2;M[d]=2;M[o]=2;M[l]=2;
av=VV.time()
while True:
    E.fill((100,100,100))
    if not p3(M) and KK==1:
        zT=RR.randint(0,9);
        if zT%10<1:TT=0;Gl(5,6,15,16)
