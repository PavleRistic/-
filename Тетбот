            A=a*10+9
            while A>10:
                if M[A]==1 and M[A]!=2:M[A]=M[A-10];
                A-=1
    B=[];E=0;A=0;
    for a in range(10):
        b=sk(a-5);
        for l in b:
            if l!=None:B.append(l)
